{
    "name": "IS2B",
    "description": " IS2B",
    "version": "1.0.0",
    "keywords": [
        "cakephp"
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "config": {
        "vendor-dir": "../plugins"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../is2b/www/plugins/*/",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../../is2b/www/app/Plugin/*/",
            "options": {
                "symlink": false
            }
        }
    ],
    "require-dev": {
        "cakephp/debug_kit" : "^2.2.0",
        "Currency": "*",
        "Entities": "*",
        "Dictionaries": "*",
        "ComplexCategories": "*",
        "Users": "*",
        "FileManager": "*",
        "Agency": "*",
        "ObjectTemplates": "*",
        "Fastadmin": "*",
        "DatetimePicker": "*",
        "ActivityStatistics": "*",
        "Reports": "*",
        "MarketAnalysis": "*",
        "Histories": "*",
        "FullCalendar": "*",
        "News": "*",
        "YandexMaps": "*",
        "Locations": "*",
        "Collations": "*",
        "Tasks": "*",
        "ModelComments": "*",
        "UserCalls": "*",
        "ScopeProfile": "*",
        "Contractors": "*",
        "Events": "*",
        "ExtraData": "*",
        "Source": "*",
        "DefaultProject": "*",
        "Documents": "*",
        "Deals": "*",
        "Services": "*",
        "Dompdf": "*",
        "Companies": "*",
        "CeniyMetr": "*"
    }
}